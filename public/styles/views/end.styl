@import "../utils/variables";
@import "../utils/mixins";

.end-view {
    padding 24px 12px;
    overflow-y : auto;

    h1 {
      display : block;
      text-align : center;
      margin 24px 8px;
    }

    .desc {
        margin 36px 12px;
        text-align center;
    }

    .btn-back,
    .btn-home {
        border-radius : 4px;
        border: 1px solid $color-black-normal;
        padding 8px 12px;
        text-align : center;
        width : 45%;
        margin-left : 3%;
        display : inline-block;
    }
}